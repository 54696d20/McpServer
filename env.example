# MCP Configuration
# Copy this file to .env and update the paths for your system

# Ollama model name (e.g., llama2:7b, mistral:7b, codellama:7b)
MCP_OLLAMA_MODEL=llama2:7b

# Path to your MCP server executable
MCP_SERVER_PATH=/path/to/McpServer.Server/bin/Debug/net9.0/McpServer.Server.exe

# Optional: Customize client and server names/versions
MCP_CLIENT_NAME=McpClient
MCP_CLIENT_VERSION=v1.0.0
MCP_SERVER_NAME=SimpleMcp.Server
MCP_SERVER_VERSION=v1.0.0 